{
	"name": "Obscuria",
	"type": "dark",
	"semanticHighlighting": true,
	"colors": {
		// Base Colors
		"focusBorder": "#665D6F",
		"foreground": "#EDEDDE",
		"errorForeground": "#ff5555",
		
		// ActivityBar
		"activityBar.background": "#26242e",
		"activityBar.foreground": "#EDEDDE",
		"activityBar.inactiveForeground": "#857A8F",
		"activityBar.activeBorder": "#b42299",
		"activityBar.activeFocusBorder": "#b42299",
		"activityBar.activeBackground": "#d228b315",
		"activityBarBadge.background": "#b42299",
		"activityBarBadge.foreground": "#EDEDDE",

		// Button Control
		"button.background": "#631c56",
		"button.foreground": "#EDEDDE",
		"button.border": "#e6e6e623",

		// Dropdown Control
		"dropdown.background": "#1A191F",
		"dropdown.foreground": "#EDEDDE",

		// Input Control
		"input.background": "#1A191F",
		"input.border": "#0F0F0F",
		"input.foreground": "#EDEDDE",
		"input.placeholderForeground": "#80748B",
		"inputValidation.errorBackground": "#4c1a1ae8",
		"inputValidation.errorBorder": "#6A2424",
		
		// Side Bar
		"sideBar.background": "#151419",
		"sideBar.foreground": "#CCCCCC",
		"sideBarTitle.foreground": "#EDEDDE",
		"sideBarSectionHeader.background": "#1F1E24",
		"sideBarSectionHeader.border": "#0F0F0F",

		// Editor Colors
		"editor.background": "#1A191F",
		"editor.foreground": "#EDEDDE",
		"editorLineNumber.foreground": "#5C5464",
		"editorLineNumber.activeForeground": "#ffffff",
		"editorGroupHeader.tabsBackground": "#1D1B23",
		"editorWidget.background": "#1A191F",
		"editorWidget.border": "#42384D",
		"editor.selectionBackground": "#631c56d3",
		"editor.inactiveSelectionBackground": "#631c5662",
		"editor.lineHighlightBorder": "#42384d50",
		"editorIndentGuide.background1": "#26202C",
		"editorIndentGuide.activeBackground1": "#574A64",

		// List Colors
		"list.activeSelectionBackground": "#631c56",
		"list.inactiveSelectionBackground": "#26202C",
		"list.dropBackground": "#631c56",
		"list.hoverBackground": "#26252D",

		// Menu Colors
		"menu.background": "#1A191F",
		"menu.foreground": "#CCCCCC",
		"menu.selectionForeground": "#EDEDDE",

		// Minimap Colors
		"minimap.background": "#26252d27",
		
		// Notification Colors
		"notificationCenterHeader.background": "#1F1E24",
		"notifications.background": "#1A191F",
		"notifications.border": "#42384D",
		"notificationToast.border": "#42384D",

		// ScrollBarSlider Colors
		"scrollbarSlider.background": "#9d8bca20",
		"scrollbarSlider.activeBackground": "#9d8bca50",
		"scrollbarSlider.hoverBackground": "#9d8bca30",

		// StatusBar Colors
		"statusBar.background": "#1A191F",
		"statusBar.noFolderBackground": "#1A191F",
		"statusBar.debuggingBackground": "#DC4141",
		"statusBar.border": "#42384D",
		"statusBar.focusBorder": "#4C4059",
		"statusBarItem.hoverBackground": "#42384D",
		"statusBarItem.remoteBackground": "#631c56",
		"statusBarItem.remoteHoverBackground": "#9B2C87",

		// Settings Colors
		"settings.modifiedItemIndicator": "#b42299",
		"settings.headerForeground": "#EDEDDE",
		"settings.textInputForeground": "#EDEDDE",
		"settings.numberInputForeground": "#EDEDDE",
		"settings.dropdownBorder": "#0F0F0F",
		"settings.numberInputBorder": "#0F0F0F",
		"settings.checkboxBorder": "#0F0F0F",
		"settings.checkboxForeground": "#b42299",
		"settings.rowHoverBackground": "#26252d7c",

		// TextLink Colors
		"textLink.foreground": "#b42299",
		"textLink.activeForeground": "#DF53C5",

		// TitleBar Color
		"titleBar.activeBackground": "#1D1B23",
		
		// Tab Colors
		"tab.inactiveBackground": "#1F1E24",
		"tab.border": "#42384D",
	},
	"tokenColors": [
		// GENERAL ==============================================================
		{
			"scope": "emphasis",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "strong",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": "header",
			"settings": {
				"foreground": "#A164F6"
			}
		},

		// DIFFS ================================================================
		{
			"scope": [
				"meta.diff",
				"meta.diff.header"
			],
			"settings": {
				"foreground": "#76728D"
			}
		},
		{
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#5591C9"
			}
		},
		{
			"scope": "markup.deleted",
			"settings": {
				"foreground": "#DC4141"
			}
		},
		{
			"scope": "markup.changed",
			"settings": {
				"foreground": "#FFB86C"
			}
		},
		{
			"scope": "invalid",
			"settings": {
				"foreground": "#DC4141",
				"fontStyle": "italic underline"
			}
		},
		{
			"scope": "invalid.deprecated",
			"settings": {
				"foreground": "#EDEDDE",
				"fontStyle": "italic underline"
			}
		},
		{
			"scope": "entity.name.filename",
			"settings": {
				"foreground": "#aa9a60"
			}
		},
		{
			"scope": "markup.error",
			"settings": {
				"foreground": "#DC4141"
			}
		},

		// MARKDOWN / RST / PROSE
		{
			"name": "Underlined markup",
			"scope": "markup.underline",
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"name": "Bold markup",
			"scope": "markup.bold",
			"settings": {
				"foreground": "#FFB86C",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markup headings",
			"scope": "markup.heading",
			"settings": {
				"foreground": "#A164F6",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markup italic",
			"scope": "markup.italic",
			"settings": {
				"foreground": "#aa9a60",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Bullets, lists (prose)",
			"scope": [
				"beginning.punctuation.definition.list.markdown",
				"beginning.punctuation.definition.quote.markdown",
				"punctuation.definition.link.restructuredtext"
			],
			"settings": {
				"foreground": "#8BE9FD",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Inline code (prose)",
			"scope": [
				"markup.inline.raw",
				"markup.raw.restructuredtext"
			],
			"settings": {
				"foreground": "#5591C9"
			}
		},
		{
			"name": "Links (prose)",
			"scope": [
				"markup.underline.link",
				"markup.underline.link.image"
			],
			"settings": {
				"foreground": "#8BE9FD"
			}
		},
		{
			"name": "Link text, image alt text (prose)",
			"scope": [
				"meta.link.reference.def.restructuredtext",
    			"punctuation.definition.directive.restructuredtext",
    			"tring.other.link.description",
				"string.other.link.title"
			],
			"settings": {
				"foreground": "#C955B4"
			}
		},
		{
			"name": "Blockquotes (prose)",
			"scope": [
				"entity.name.directive.restructuredtext",
				"markup.quote"
			],
			"settings": {
				"foreground": "#AA9A60",
				"fontStyle": "italic"
			}
		},
		{
			"name": "horizontal rule (prose)",
			"scope": "meta.separator.markdown",
			"settings": {
				"foreground": "#76728D"
			}
		},
		{
			"name": "Code blocks",
			"scope": [
				"fenced_code.block.language",
				"markup.raw.inner.restructuredtext",
				"markup.fenced_code.block.markdown punctuation.definition.markdown"
			],
			"settings": {
				"foreground": "#5591C9"
			}
		},
		{
			"name": "Prose constants",
			"scope": "punctuation.definition.constant.restructuredtext",
			"settings": {
				"foreground": "#A164F6"
			}
		},
		{
			"name": "Braces in markdown headings",
			"scope": [
				"markup.heading.markdown punctuation.definition.string.begin",
				"markup.heading.markdown punctuation.definition.string.end"
			],
			"settings": {
				"foreground": "#A164F6"
			}
		},
		{
			"name": "Braces in markdown paragraphs",
			"scope": [
				"meta.paragraph.markdown punctuation.definition.string.begin",
				"meta.paragraph.markdown punctuation.definition.string.end"
			],
			"settings": {
				"foreground": "#EDEDDE"
			}
		},
		{
			"name": "Braces in markdown blockquotes",
			"scope": [
				"markup.quote.markdown meta.paragraph.markdown punctuation.definition.string.begin",
				"markup.quote.markdown meta.paragraph.markdown punctuation.definition.string.end"
			],
			"settings": {
				"foreground": "#AA9A60"
			}
		},

		// CLASSES ==============================================================
		{
			"name": "User-defined class names",
			"scope": [
				"entity.name.type.class",
				"entity.name.class"
			],
			"settings": {
				"foreground": "#8BE9FD"
			}
		},
		{
			"name": "this, super, self, etc.",
			"scope": [
				"keyword.expressions-and-types.swift",
				"keyword.other.this",
				"variable.language",
				"variable.language punctuation.definition.variable.php",
				"variable.other.readwrite.instance.ruby",
				"variable.parameter.function.language.special"
			],
			"settings": {
				"foreground": "#A164F6",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Inherited classes",
			"scope": "entity.other.inherited-class",
			"settings": {
				"foreground": "#8BE9FD",
				"fontStyle": "italic"
			}
		},

		// COMMENTS =============================================================
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment",
				"unused.comment",
				"wildcard.comment"
			],
			"settings": {
				"foreground": "#76728D"
			}
		},
		{
			"name": "JSDoc-style keywords",
			"scope": [
				"comment keyword.codetag.notation",
				"comment.block.documentation keyword",
				"comment.block.documentation storage.type.class"
			],
			"settings": {
				"foreground": "#C955B4"
			}
		},
		{
			"name": "JSDoc-style types",
			"scope": "comment.block.documentation entity.name.type",
			"settings": {
				"foreground": "#8BE9FD",
				"fontStyle": "italic"
			}
		},
		{
			"name": "JSDoc-style type brackets",
			"scope": "comment.block.documentation entity.name.type punctuation.definition.bracket",
			"settings": {
				"foreground": "#8BE9FD"
			}
		},
		{
			"name": "JSDoc-style comment parameters",
			"scope": "comment.block.documentation variable",
			"settings": {
				"foreground": "#FFB86C",
				"fontStyle": "italic"
			}
		},

		// CONSTANTS =============================================================
		{
			"name": "Constants",
			"scope": [
				"constant",
				"variable.other.constant"
			],
			"settings": {
				"foreground": "#A164F6"
			}
		},
		{
			"name": "Constant escape sequences",
			"scope": [
				"constant.character.escape",
				"constant.character.string.escape",
				"constant.regexp"
			],
			"settings": {
				"foreground": "#C955B4"
			}
		},

		// ENTITIES ==============================================================
		{
			"name": "HTML tags",
			"scope": [
				"entity.name.tag"
			],
			"settings": {
				"foreground": "#C955B4"
			}
		},
		{
			"name": "CSS attribute parent selectors ('&')",
			"scope": [
				"entity.other.attribute-name.parent-selector"
			],
			"settings": {
				"foreground": "#C955B4"
			}
		},
		{
			"name": "HTML/CSS attribute names",
			"scope": [
				"entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#5591C9"
			}
		},

		// STRINGS =========================================================
		{
			"name": "Strings",
			"scope": [
				"string"
			],
			"settings": {
				"foreground": "#aa9a60"
			}
		},
		{
			"name": "String quotes (temporary vscode fix)",
			"scope": [
				"punctuation.definition.string.begin",
				"punctuation.definition.string.end"
			],
			"settings": {
				"foreground": "#aa9a60"
			}
		},
		{
			"name": "Property quotes (temporary vscode fix)",
			"scope": [
				"punctuation.support.type.property-name.begin",
				"punctuation.support.type.property-name.end"
			],
			"settings": {
				"foreground": "#aa9a60"
			}
		},
		{
			"name": "Docstrings",
			"scope": [
				"string.quoted.docstring.multi",
				"string.quoted.docstring.multi.python punctuation.definition.string.begin",
				"string.quoted.docstring.multi.python punctuation.definition.string.end",
				"string.quoted.docstring.multi.python constant.character.escape"
			],
			"settings": {
				"foreground": "#76728D"
			}
		}
	]
}
