{
	"name": "Obscuria",
	"type": "dark",
	"semanticHighlighting": true,
	"colors": {
		// Base Colors
		"focusBorder": "#665D6F",
		"foreground": "#EDEDDE",
		"errorForeground": "#ff5555",
		
		// ActivityBar
		"activityBar.background": "#26242e",
		"activityBar.foreground": "#EDEDDE",
		"activityBar.inactiveForeground": "#857A8F",
		"activityBar.activeBorder": "#b42299",
		"activityBar.activeFocusBorder": "#b42299",
		"activityBar.activeBackground": "#d228b315",
		"activityBarBadge.background": "#b42299",
		"activityBarBadge.foreground": "#EDEDDE",

		// Button Control
		"button.background": "#631c56",
		"button.foreground": "#EDEDDE",
		"button.border": "#e6e6e623",

		// Dropdown Control
		"dropdown.background": "#1A191F",
		"dropdown.foreground": "#EDEDDE",

		// Input Control
		"input.background": "#1A191F",
		"input.border": "#0F0F0F",
		"input.foreground": "#EDEDDE",
		"input.placeholderForeground": "#80748B",
		"inputValidation.errorBackground": "#4c1a1ae8",
		"inputValidation.errorBorder": "#6A2424",
		
		// Side Bar
		"sideBar.background": "#151419",
		"sideBar.foreground": "#CCCCCC",
		"sideBarTitle.foreground": "#EDEDDE",
		"sideBarSectionHeader.background": "#1F1E24",
		"sideBarSectionHeader.border": "#0F0F0F",

		// Editor Colors
		"editor.background": "#1A191F",
		"editor.foreground": "#EDEDDE",
		"editorLineNumber.foreground": "#5C5464",
		"editorLineNumber.activeForeground": "#ffffff",
		"editorGroupHeader.tabsBackground": "#1D1B23",
		"editorWidget.background": "#1A191F",
		"editorWidget.border": "#42384D",
		"editor.selectionBackground": "#631c56d3",
		"editor.inactiveSelectionBackground": "#631c5662",
		"editor.lineHighlightBorder": "#42384d50",
		"editorIndentGuide.background1": "#26202C",
		"editorIndentGuide.activeBackground1": "#574A64",

		// List Colors
		"list.activeSelectionBackground": "#631c56",
		"list.inactiveSelectionBackground": "#26202C",
		"list.dropBackground": "#631c56",
		"list.hoverBackground": "#26252D",

		// Menu Colors
		"menu.background": "#1A191F",
		"menu.foreground": "#CCCCCC",
		"menu.selectionForeground": "#EDEDDE",

		// Minimap Colors
		"minimap.background": "#26252d27",
		
		// Notification Colors
		"notificationCenterHeader.background": "#1F1E24",
		"notifications.background": "#1A191F",
		"notifications.border": "#42384D",
		"notificationToast.border": "#42384D",

		// ScrollBarSlider Colors
		"scrollbarSlider.background": "#9d8bca20",
		"scrollbarSlider.activeBackground": "#9d8bca50",
		"scrollbarSlider.hoverBackground": "#9d8bca30",

		// StatusBar Colors
		"statusBar.background": "#1A191F",
		"statusBar.noFolderBackground": "#1A191F",
		"statusBar.debuggingBackground": "#DC4141",
		"statusBar.border": "#42384D",
		"statusBar.focusBorder": "#4C4059",
		"statusBarItem.hoverBackground": "#42384D",
		"statusBarItem.remoteBackground": "#631c56",
		"statusBarItem.remoteHoverBackground": "#9B2C87",

		// Settings Colors
		"settings.modifiedItemIndicator": "#b42299",
		"settings.headerForeground": "#EDEDDE",
		"settings.textInputForeground": "#EDEDDE",
		"settings.numberInputForeground": "#EDEDDE",
		"settings.dropdownBorder": "#0F0F0F",
		"settings.numberInputBorder": "#0F0F0F",
		"settings.checkboxBorder": "#0F0F0F",
		"settings.checkboxForeground": "#b42299",
		"settings.rowHoverBackground": "#26252d7c",

		// TextLink Colors
		"textLink.foreground": "#b42299",
		"textLink.activeForeground": "#DF53C5",

		// TitleBar Color
		"titleBar.activeBackground": "#1D1B23",
		
		// Tab Colors
		"tab.inactiveBackground": "#1F1E24",
		"tab.border": "#42384D",
	},
	"tokenColors": [
		// COMMENTS =============================================================
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment",
				"unused.comment",
				"wildcard.comment"
			],
			"settings": {
				"foreground": "#76728D"
			}
		},

		// ENTITIES ==============================================================
		{
			"name": "HTML tags",
			"scope": [
				"entity.name.tag"
			],
			"settings": {
				"foreground": "#C955B4"
			}
		},
		{
			"name": "CSS attribute parent selectors ('&')",
			"scope": [
				"entity.other.attribute-name.parent-selector"
			],
			"settings": {
				"foreground": "#C955B4"
			}
		},
		{
			"name": "HTML/CSS attribute names",
			"scope": [
				"entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#5591C9"
			}
		},

		// STRINGS =========================================================
		{
			"name": "Strings",
			"scope": [
				"string"
			],
			"settings": {
				"foreground": "#C2B070"
			}
		},
		{
			"name": "String quotes (temporary vscode fix)",
			"scope": [
				"punctuation.definition.string.begin",
				"punctuation.definition.string.end"
			],
			"settings": {
				"foreground": "#C2B070"
			}
		},
		{
			"name": "Property quotes (temporary vscode fix)",
			"scope": [
				"punctuation.support.type.property-name.begin",
				"punctuation.support.type.property-name.end"
			],
			"settings": {
				"foreground": "#C2B070"
			}
		},
		{
			"name": "Docstrings",
			"scope": [
				"string.quoted.docstring.multi",
				"string.quoted.docstring.multi.python punctuation.definition.string.begin",
				"string.quoted.docstring.multi.python punctuation.definition.string.end",
				"string.quoted.docstring.multi.python constant.character.escape"
			],
			"settings": {
				"foreground": "#76728D"
			}
		}
	]
}
